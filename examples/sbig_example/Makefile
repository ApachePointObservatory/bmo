####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: ccdOpsLite.pro  /usr/lib/qt3/mkspecs/default/qmake.conf /usr/lib/qt3/lib/libqt.prl
	$(QMAKE) -o Makefile ccdOpsLite.pro
qmake: 
	@$(QMAKE) -o Makefile ccdOpsLite.pro

dist: 
	@mkdir -p .obj/ccdOpsLite && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/ccdOpsLite/ && $(COPY_FILE) --parents images/filenew images/fileopen images/filesave images/print images/undo images/redo images/editcut images/editcopy images/editpaste images/searchfind .obj/ccdOpsLite/ && $(COPY_FILE) --parents mainform.ui.h cameracontrol.ui.h contrastform.ui.h imageinfoform.ui.h linksetupform.ui.h .obj/ccdOpsLite/ && ( cd `dirname .obj/ccdOpsLite` && $(TAR) ccdOpsLite.tar ccdOpsLite && $(GZIP) ccdOpsLite.tar ) && $(MOVE) `dirname .obj/ccdOpsLite`/ccdOpsLite.tar.gz . && $(DEL_FILE) -r .obj/ccdOpsLite

